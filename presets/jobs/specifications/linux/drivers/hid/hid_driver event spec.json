{
  "3.14": {
    "environment processes": {
      "hid_driver_scenario": {
        "actions": {
          "alloc_device": {
            "comment": "Allocate memory and initialize new USB interface.",
            "statements": [
              "%resource% = $ALLOC(%resource%);",
              "%hid_id% = & %hid_driver%->id_table[0];"
            ]
          },
          "callback": {
            "callback": "%callback%",
            "parameters": [
              "%resource%"
            ]
          },
          "deregister": {
            "condition": [
              "%hid_driver% == $ARG1"
            ],
            "parameters": [
              "%hid_driver%"
            ]
          },
          "failed_probe": {
            "comment": "Association of driver-specific data with the USB interface has been failed.",
            "condition": [
              "%probe_retval% != 0"
            ]
          },
          "free_device": {
            "comment": "Free allocated memory for USB interface.",
            "statements": [
              "$FREE(%resource%);"
            ]
          },
          "manage_hid": {
            "comment": "Manage particular interface on a device.",
            "process": "[probe].(<probe_success>.([suspend].([resume] | [reset_resume]) | [callback] | <skip>).[remove] | <failed_probe>).{manage_hid} | <free_device>.(deregister)"
          },
          "probe": {
            "callback": "%hid_driver.probe%",
            "callback return value": "%probe_retval%",
            "comment": "Associate driver-specific data with the USB interface.",
            "parameters": [
              "%resource%", "%hid_id%"
            ],
            "post-call": [
              "%probe_retval% = ldv_post_probe(%probe_retval%);"
            ],
            "pre-call": [
              "ldv_pre_probe();"
            ]
          },
          "probe_success": {
            "comment": "Check that interface has been probed successfully.",
            "condition": [
              "%probe_retval% == 0"
            ]
          },
          "register": {
            "comment": "Begin USB callbacks invocation scenario.",
            "parameters": [
              "%hid_driver%"
            ]
          },
          "remove": {
            "callback": "%hid_driver.remove%",
            "comment": "USB interface is no more accessible.",
            "parameters": [
              "%hid_driver%"
            ]
          },
          "reset_resume": {
            "callback": "%hid_driver.reset_resume%",
            "comment": "Device is about to be resumed.",
            "parameters": [
              "%resource%"
            ]
          },
          "resume": {
            "callback": "%hid_driver.resume%",
            "comment": "Device is about to be resumed.",
            "parameters": [
              "%resource%"
            ]
          },
          "skip": {
            "comment": "Do not suspend or reset device."
          },
          "suspend": {
            "callback": "%hid_driver.suspend%",
            "comment": "Device is about to be suspended.",
            "parameters": [
              "%resource%"
            ]
          }
        },
        "comment": "Invoke usb callbacks.",
        "labels": {
          "callback": {
            "callback": true
          },
          "hid_driver": {
            "hid_driver": true,
            "interface": [
              "hid_driver.hid_driver"
            ]
          },
          "probe_retval": {
            "declaration": "int a",
            "value": "ldv_undef_int()"
          },
          "resource": {
            "resource": true
          },
          "hid_id": {
            "interface": [
              "hid_driver.hid_id"
            ]
          }
        },
        "process": "(!register).(<alloc_device>.{manage_hid})",
        "reference": true
      }
    },
    "functions models": {
      "hid_unregister_driver": {
        "actions": {
          "assign": {
            "comment": "Get callbacks to deregister provided within 'usb_driver' argument.",
            "statements": [
              "%hid_driver% = $ARG1;"
            ]
          },
          "deregister": {
            "parameters": [
              "%hid_driver%"
            ]
          }
        },
        "comment": "Deregister USB callbacks.",
        "labels": {
          "hid_driver": {
            "hid_driver": true,
            "interface": [
              "hid_driver.hid_driver"
            ],
            "parameter": true
          }
        },
        "process": "<assign>.[deregister]",
        "reference": true
      },
      "__hid_register_driver": {
        "actions": {
          "assign": {
            "comment": "Get callbacks to register provided within 'usb_driver' argument.",
            "statements": [
              "%hid_driver% = $ARG1;"
            ]
          },
          "fail": {
            "comment": "Fail USB callbacks registration.",
            "statements": [
              "return ldv_undef_int_negative();"
            ]
          },
          "register": {
            "parameters": [
              "%hid_driver%"
            ]
          },
          "success": {
            "comment": "Registration of usb_driver callbacks has been finished successfully.",
            "statements": [
              "return 0;"
            ]
          }
        },
        "comment": "Register USB callbacks.",
        "labels": {
          "hid_driver": {
            "hid_driver": true,
            "interface": [
              "hid_driver.hid_driver"
            ],
            "parameter": true
          }
        },
        "process": "<assign>.[register].<success> | <fail>",
        "reference": true
      }
    }
  }
}