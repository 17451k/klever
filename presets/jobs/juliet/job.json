{
  "Class": "Verification of userspace programs",
  "Common": {
    "program": "Juliet",
    "version": "1.3",
    "fragmentation set": "Juliet",
    "specifications set": "Linux 3.14 (base)",
    "requirements": ["juliet:memory safety"],
    "add targets": ["**"],
    "exclude targets": [
      "testcasesupport/io.c",
      "testcasesupport/std_thread.c"
    ]
  },
  "Sub-jobs": [
      {
          "build base": "build bases/juliet-omitbad",
          "ideal verdicts": [
            {"ideal verdict": "safe"}
          ]
      },
      {
          "build base": "build bases/juliet-omitgood",
          "ideal verdicts": [
            {"ideal verdict": "unsafe"}
          ]
      }
  ]
}