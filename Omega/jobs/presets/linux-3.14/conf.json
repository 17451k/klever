{
  "Linux kernel": {
    "source": "linux-3.14",
    "configuration": "allmodconfig",
    "build kernel": false,
    "modules_": ["all"],
    "modules_": ["drivers/usb/serial"],
    "modules": ["drivers/usb/serial/usbserial.ko"],
    "modules_": ["drivers/usb/core/usbcore.ko", "drivers/usb/usb-common.ko"]
  },
  "LKVOG strategy": {
    "name": "separate modules"
  },
  "rule specifications": ["linux:mutex:as"],
  "rule specifications_": ["32", "linux:spinlock"],
  "rule specifications DB": "rule specs.json",
  "VTG strategy": {
    "name": "all bug kinds merge",
    "verifier": {
      "name": "CPAchecker",
      "version": "1.4",
      "options": [
        {"-ldv": ""}
      ]
    },
    "resource limits": {
      "wall time": 1000,
      "CPU time": 1000,
      "memory size": 1000000000,
      "CPU model": null,
      "number of CPU cores": 1,
      "result size": 1000000
    },
    "merge source files": true
  }
}
