{
  "Common": {
    "Linux kernel": {
      "source": "linux-stable",
      "configuration": "allmodconfig",
      "build kernel": false
    },
    "LKVOG strategy": {
      "name": "separate modules"
    },
    "rule specifications DB": "rule specs.json",
    "VTG strategy": {
      "name": "all bug kinds merge",
      "verifier": {
        "name": "CPAchecker",
        "version": "19280",
        "options": [
          {"-ldv": ""}
        ]
      },
      "resource limits": {
        "wall time": 1000000,
        "CPU time": 900000,
        "memory size": 10000000000,
        "CPU model": null,
        "number of CPU cores": 1,
        "result size": 1000000
      },
      "merge source files": true
    }
  },
  "Sub-jobs": [
    {
      "Linux kernel": {
        "Git repository": {
          "commit": "bcb4a75~"
        },
        "modules": ["drivers/hid/hid.ko"]
      },
      "rule specifications": ["linux:usb:gadget"],
      "ideal verdict": "unsafe"
    },
    {
      "Linux kernel": {
        "Git repository": {
          "commit": "e4c7f25~"
        },
        "modules": ["drivers/net/usb/kaweth.ko"]
      },
      "rule specifications": ["linux:alloc2"],
      "ideal verdict": "unsafe"
    },
    {
