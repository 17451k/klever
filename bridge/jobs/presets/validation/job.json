      "source": "linux-stable",
      "configuration": "allmodconfig",
      "build kernel": false
    },
    "LKVOG strategy": {
      "name": "separate modules"
    },
    "rule specifications DB": "rule specs.json",
    "VTG strategy": {
      "name": "all bug kinds merge",
      "verifier": {
        "name": "CPAchecker",
        "version": "18921",
        "options": [
          {"-ldv": ""}
