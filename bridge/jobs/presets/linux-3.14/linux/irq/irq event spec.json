          "data": {
            "resource": true,
            "interface": [
              "interrupt.data"
            ],
            "parameter": true
          }
        },
        "process": "<assign>.[irq_register] | <none>",
        "actions": {
          "irq_register": {
            "parameters": [
              "%line%",
              "%callback%",
              "%thread%",
              "%data%"
            ]
          },
          "none": {
            "condition": [
              "$res != 0"
            ]
          },
          "assign": {
            "condition": [
              "$res == 0"
            ],
            "statements": [
              "%line% = $arg1;",
              "%callback% = $arg2;",
              "%thread% = $arg3;",
              "%data% = $arg6;"
            ]
          }
        }
      },
