before: file ("$this")
{
int ldv_usb_register(void);
int ldv_register_netdev(void);
}

/* Nondeterministically change state after call to usb_register(). */
around: define(usb_register(driver))
{
	ldv_usb_register()
}

/* Nondeterministically change state after call to register_netdev(). */
after: call(int register_netdev(..))
{
	return ldv_register_netdev();
}
