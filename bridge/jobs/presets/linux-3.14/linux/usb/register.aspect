before: file ("$this")
{
int ldv_usb_register(void);
int ldv_register_netdev(void);
void ldv_check_return_value_probe(int retval);
}

/* Nondeterministically change state after call to usb_register(). */
around: define(usb_register(driver))
{
	ldv_usb_register()
}

before: call(int ldv_post_probe(..))
{
	ldv_check_return_value_probe($arg1);
}