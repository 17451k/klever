          "container": true,
          "interface": ["timer.timer_list"]
        }
      },
      "process": "(!instance_register).[callback].(instance_deregister)",
      "actions": {
        "instance_register": {
          "parameters": [
            "%container%"
          ]
        },
        "instance_deregister": {
          "condition": ["%container% == $ARG1"],
          "parameters": [
            "%container%"
          ]
        },
        "callback": {
