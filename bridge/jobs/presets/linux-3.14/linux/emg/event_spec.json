    "seq_instance": {
      "labels": {
        "ops": {
          "container": true,
          "interface": ["seq_operations.seq_operations"]
        },
        "seq_file": {
          "resource": true,
          "interface": ["seq_operations.seq_file"]
        },
        "started": {
          "signature": "int a",
          "value": "0"
        }
      },
      "process": "(!register).{do}",
      "actions": {
        "do": {
          "process": "<started>.([stop].<post_stop> | [show] | [next]).{do} | <not_started>.([start].<post_start>.{do} | (deregister))"
        },
        "deregister": {
          "parameters": ["%seq_file%"]
        },
        "register": {
          "parameters": ["%seq_file%", "%ops%"]
        },
        "post_start": {
          "statements": [
            "%started% = 1;"
          ]
        },
        "post_stop": {
          "statements": [
            "%started% = 0;"
          ]
        },
        "started": {
          "condition": [
            "%started% == 1"
          ]
        },
        "not_started": {
          "condition": [
            "%started% == 0"
          ]
        },
        "start": {
          "callback": "%ops.start%",
          "parameters": ["%seq_file%"]
        },
        "stop": {
          "callback": "%ops.stop%",
