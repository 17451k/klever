{
  "Linux 3.14 (base)": {
    "kernel model": {},
    "environment processes": {
      "random_proto_scenario": {
        "headers": ["linux/poll.h"],
        "comment": "Scenario with random invocation of callbacks.",
        "labels": {
          "container": {
            "interface": ["proto.socket"],
            "container": true,
            "resource": true
          },
          "callback": {
            "callback": true
          }
        },
        "process": "(!register).{call}",
        "actions": {
          "call": {
            "comment": "Prepare to call a random callback or deregister the callbacks.",
            "process": "[callback].{call} | (deregister)"
          },
          "register": {
            "parameters": [
              "%container%"
            ]
          },
          "deregister": {
            "condition": [
              "%container% == $ARG1"
            ],
            "parameters": [
              "%container%"
            ]
          },
          "callback": {
            "comment": "Call any random callback.",
            "callback": "%callback%",
            "parameters": [
              "%container%"
            ]
          }
        }
      }
    }
  }
}