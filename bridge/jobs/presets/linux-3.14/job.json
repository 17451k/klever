{
  "Linux kernel": {
    "source": "linux-3.5.tar.bz2",
    "configuration": "allmodconfig",
    "modules": ["drivers/net/usb/kaweth.ko"]
  },
  "LKVOG strategy": {"name": "separate modules"},
  "maximum verification object size": 30000,
  "rule specifications DB": "rule specs.json",
  "rule specifications": ["linux:alloc:spin lock", "linux:mutex", "linux:module"],
  "unite rule specifications": false,
  "RSG strategy": "instrumentation",
  "VTG strategy": {
    "name": "single bug type",
    "verifier": {
      "name": "CPAchecker",
      "version": "20376",
      "options": [{"-ldv": ""}]
    },
    "resource limits": {
      "wall time": 1000000,
      "CPU time": 900000,
      "memory size": 10000000000,
      "CPU model": null,
      "number of CPU cores": 1,
      "result size": 1000000
    },
    "merge source files": true
  }
}
