{
  "Linux kernel": {
    "source": "linux-3.14",
    "configuration": "allmodconfig",
    "architecture": "x86_64",
    "modules": ["drivers/net/usb/kaweth.ko"]
  },
  "LKVOG strategy": {"name": "separate modules"},
  "maximum verification object size": 30000,
  "rule specifications DB": "rule specs.json",
  "verifier profiles DB": "verifier profiles.json",
  "specifications set": "Linux 3.14 (base)",
  "rule specifications": ["linux:alloc:spinlock", "linux:kernel:locking:mutex", "linux:kernel:module"],
  "Module extractor": {
    "name": "Linux kernel"
  },
  "aspect file": "linux/emg/source_analysis.aspect.tmpl",
  "allowed macros": [
    "module_init",
    "early_initcall",
    "pure_initcall",
    "core_initcall",
    "core_initcall_sync",
    "postcore_initcall",
    "postcore_initcall_sync",
    "arch_initcall",
    "arch_inicall_sync",
    "subsys_initcall",
    "subsys_initcall_sync",
    "fs_initcall",
    "fs_initcall_sync",
    "rootfs_initcall",
    "device_initcall",
    "device_initcall_sync",
    "late_initcall",
    "late_initcall_sync",
    "console_initcall",
    "security_initcall",
    "module_exit"
  ]
}
