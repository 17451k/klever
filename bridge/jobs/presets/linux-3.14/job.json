{
  "Linux kernel": {
    "source": "linux-3.14.tar.xz",
    "configuration": "allmodconfig",
    "build kernel": false,
    "modules": ["drivers/block/cciss.ko"]
  },
  "LKVOG strategy": {
    "name": "separate modules"
  },
  "rule specifications DB": "rule specs.json",
  "rule specifications": ["linux:mutex"],
  "VTG strategy": {
    "name": "all bug kinds merge",
    "verifier": {
      "name": "CPAchecker",
      "version": "1.4",
      "options": [
        {"-ldv": ""}
      ]
    },
    "resource limits": {
      "wall time": 1000000,
      "CPU time": 900000,
      "memory size": 10000000000,
      "CPU model": null,
      "number of CPU cores": 1,
      "result size": 1000000
    },
    "merge source files": true
  }
}
