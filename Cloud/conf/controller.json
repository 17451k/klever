{
  "Omega": {
    "name": "localhost:8998",
    "user": "user",
    "password": "password"
  },
  "client-controller": {
    "consul": "tools/consul",
    "script checks": [
      {
        "name": "docker",
        "active": false,
        "interval": "15s"
      },
      {
        "name": "resources",
        "active": true,
        "interval": "5s"
      },
      {
        "name": "node",
        "active": true,
        "interval": "5s"
      },
      {
        "name": "schedulers",
        "active": true,
        "interval": "5s"
      }
    ],
    "service checks": [
      {
        "id": "Omega",
        "name": "Omega on port 8998",
        "http": "http://localhost:8998/",
        "interval": "10s",
        "timeout": "1s"
      }
    ],
    "setup GUI": true,
    "consul additional opts": [
      "-bootstrap-expect=1",
      "-log-level=debug",
      "-server",
      "-retry-interval=30s"
    ]
  },
  "node configuration": {
    "bind address": "0.0.0.0",
    "available for jobs": true,
    "available for tasks": true,
    "available CPU number": 4,
    "available RAM memory": 8000000000,
    "available disk memory": 8000000000
  },
  "common": {
    "work dir": "scheduler-controller-dir",
    "keep work dir": false,
    "debug": false,
    "logging": {
      "version": 1,
      "disable_existing_loggers": false,
      "formatters": {
        "brief": {
          "format": "%(name)s %(levelname)5s> %(message)s"
        },
        "detailed": {
          "format": "%(asctime)s (%(filename)s:%(lineno)03d) %(name)s %(levelname)5s> %(message)s"
        }
      },
      "handlers": {
        "console": {
          "class": "logging.StreamHandler",
          "level": "DEBUG",
          "formatter": "brief",
          "stream": "ext://sys.stdout"
        },
        "info_file_handler": {
          "class": "logging.handlers.RotatingFileHandler",
          "level": "INFO",
          "formatter": "detailed",
          "filename": "info.log",
          "maxBytes": 10485760,
          "backupCount": 20,
          "encoding": "utf8"
        },
        "error_file_handler": {
          "class": "logging.handlers.RotatingFileHandler",
          "level": "ERROR",
          "formatter": "detailed",
          "filename": "errors.log",
          "maxBytes": 10485760,
          "backupCount": 20,
          "encoding": "utf8"
        }
      },
      "loggers": {
        "default": {
          "level": "ERROR",
          "handlers": [
            "console"
          ],
          "propagate": "no"
        }
      },
      "root": {
        "level": "DEBUG",
        "handlers": [
          "console",
          "info_file_handler",
          "error_file_handler"
        ]
      }
    }
  }
}